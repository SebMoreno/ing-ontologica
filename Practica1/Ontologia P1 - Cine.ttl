                                                                                              owl:onProperty :incluye ;
                                                                                              owl:someValuesFrom :Surround_7.1
                                                                                            ]
                                                                                          ) ;
                                                                       rdf:type owl:Class
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                          rdfs:subClassOf :Boleta_valida .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Boleta_invalida
:Boleta_invalida rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Boleta
                                                            [ rdf:type owl:Class ;
                                                              owl:complementOf :Boleta_valida
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
