###  http://dbpedia.org/ontology/Film
<http://dbpedia.org/ontology/Film> rdf:type owl:Class ;
                                   owl:equivalentClass <http://schema.org/Movie> ,
                                                       :Pelicula ,
                                                       <http://www.wikidata.org/entity/Q11424> ;
                                   rdfs:subClassOf <http://dbpedia.org/ontology/Work> .


###  http://dbpedia.org/ontology/Work
<http://dbpedia.org/ontology/Work> rdf:type owl:Class ;
                                   rdfs:label "dbo:Work"@en .


###  http://schema.org/Movie
<http://schema.org/Movie> rdf:type owl:Class .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#2D
<http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#2D> rdf:type owl:Class ;
                                                                                    rdfs:subClassOf :Pantalla ;
                                                                                    rdfs:comment "Proyección básica de una película sin ningún efecto especial" ;
                                                                                    rdfs:label "2D"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#3D
<http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#3D> rdf:type owl:Class ;
                                                                                    rdfs:subClassOf :Pantalla ;
                                                                                    rdfs:comment "Proyección de una película en 3D que requiere un formato especial en la película, y el uso de lentes 3D por parte de los espectadores" ;
                                                                                    rdfs:label "3D"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#4D
<http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#4D> rdf:type owl:Class ;
                                                                                    rdfs:subClassOf :Pantalla ;
                                                                                    rdfs:comment "Cumple el mismo objetivo que las pantallas 3D, pero incluyendo efectos especiales en la sala y en los asientos que causan una experiencia más emocionante" ;
                                                                                    rdfs:label "4D"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Afiches_promocionales
:Afiches_promocionales rdf:type owl:Class ;
                       rdfs:subClassOf :Utileria ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :publicita ;
                                         owl:allValuesFrom :Pelicula
                                       ] ;
                       rdfs:comment "Cartel que se fija en las instalaciones del cine o en sus afueras, con el fin de publicitar películas anunciadas o próximas a estrenarse" ;
                       rdfs:label "Afiches promocionales"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Asiento
:Asiento rdf:type owl:Class ;
         rdfs:subClassOf :Cine ;
         rdfs:comment "Mueble asignado para una persona dentro de una sala de cine que puede poseer diferentes características según su precio" ;
         rdfs:label "Asiento"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Asiento_4D
:Asiento_4D rdf:type owl:Class ;
            rdfs:subClassOf :Asiento ;
            rdfs:comment "Asiento capaz de moverse e inclinarse según los eventos de la película" ;
            rdfs:label "Asiento 4D"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Asiento_basico
:Asiento_basico rdf:type owl:Class ;
                rdfs:subClassOf :Asiento ;
                rdfs:comment "Asiento estándar de un cine" ;
                rdfs:label "Asiento basico"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Asiento_con_vibracion
:Asiento_con_vibracion rdf:type owl:Class ;
                       rdfs:subClassOf :Asiento ;
                       rdfs:comment "Asiento con la capacidad de vibrar dinámicamente según los sonidos y eventos de la película" ;
                       rdfs:label "Asiento con vibracion"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Asiento_preferencial
:Asiento_preferencial rdf:type owl:Class ;
                      rdfs:subClassOf :Asiento ;
                      rdfs:comment "Asiento con ubicación privilegiada dentro de la sala, que aumenta su costo" ;
                      rdfs:label "Asiento preferencial"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Audio
:Audio rdf:type owl:Class ;
       rdfs:subClassOf :Cine ;
       rdfs:comment "Equipo de sonido integrado a cada sala de cine" ;
       rdfs:label "Audio"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Baño
:Baño rdf:type owl:Class ;
      rdfs:subClassOf :Limpieza ;
      rdfs:comment "Espacio destinado para aseo personal y satisfacción de necesidades naturales" ;
      rdfs:label "Baño"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Boleta
:Boleta rdf:type owl:Class ;
        owl:equivalentClass :Boleta_digital ;
        rdfs:comment "Factura que acredita el pago de una tarifa con el fin de acceder al servicio de Cine con diferentes servicios" ;
        rdfs:label "Boleta"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Boleta_3D
:Boleta_3D rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Boleta_valida
                                                      [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :incluye ;
                                                                               owl:someValuesFrom <http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#3D>
                                                                             ]
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :incluye ;
                                                                               owl:someValuesFrom :Asiento_basico
                                                                             ]
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :incluye ;
                                                                               owl:someValuesFrom :Surround_7.1
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Boleta_valida .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Boleta_4D
:Boleta_4D rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Boleta_valida
                                                      [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :incluye ;
                                                                               owl:someValuesFrom <http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#4D>
                                                                             ]
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :incluye ;
                                                                               owl:someValuesFrom :Asiento_4D
                                                                             ]
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :incluye ;
                                                                               owl:someValuesFrom :Surround_7.1
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Boleta_valida .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Boleta_digital
:Boleta_digital rdf:type owl:Class ;
                rdfs:comment "Boleta en formato digital" ;
                rdfs:label "Boleta digital"@es .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Boleta_estandar
:Boleta_estandar rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Boleta_valida
                                                            [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :incluye ;
                                                                                     owl:someValuesFrom <http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#2D>
                                                                                   ]
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :incluye ;
                                                                                     owl:someValuesFrom :Asiento_basico
                                                                                   ]
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :incluye ;
                                                                                     owl:someValuesFrom :Estereo
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :Boleta_valida .


###  http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16/Boleta_estandar_surround
:Boleta_estandar_surround rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( :Boleta_valida
                                                                     [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty :incluye ;
                                                                                              owl:someValuesFrom <http://www.semanticweb.org/cristianm120/ontologies/2023/2/untitled-ontology-16#2D>
                                                                                            ]
                                                                                            [ rdf:type owl:Restriction ;
                                                                                              owl:onProperty :incluye ;
